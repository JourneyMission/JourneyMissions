{
	"extends": "rallycoding",
	"rules": {
		"max-len": [2, 211, 2, {
			"ignoreUrls": true,
			"ignoreComments": false
		}],
	"global-require": "off",
	"object-shorthand": "off",
	"react-native/no-unused-styles": 2,
	"react-native/split-platform-components": 2,
	"react-native/no-inline-styles": 2,
	},
	"plugins": [
		"react",
		"react-native"
	]
}